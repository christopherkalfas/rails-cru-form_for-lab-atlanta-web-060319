  
<%= form_for(@song) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    
    <%= f.collection_select(:artist_id, Artist.all, :id, :name) %>
    <%= f.collection_select(:genre_id, Genre.all, :id, :name) %>
  </div>

  

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>